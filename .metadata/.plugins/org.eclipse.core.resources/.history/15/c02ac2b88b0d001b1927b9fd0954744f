#include "buffer.h"

typedef enum {ERROR = 0, SUCCESS = !ERROR}ErrorStatus;

void BufferInit(__IO FIFO_TypeDef *buffer)
{
	buffer->count =0; //0 bytes in buffer
	buffer->in = 0; //index points to start
	buffer->out = 0; //index points to start
}

